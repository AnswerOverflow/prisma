datasource db {
  provider = "postgresql"
  url      = "postgres://prisma:prisma@localhost:5432/postgres?schema=public"
}

model profiles {
  id    String @id @db.Uuid
  users users  @relation(fields: [id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model users {
  id       String    @id @db.Uuid
  profiles profiles?
}
